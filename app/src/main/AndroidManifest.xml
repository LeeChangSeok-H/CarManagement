<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.happylife.carmanagement">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".PasswordActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".home.HomeFragment"></activity>
        <activity android:name="com.happylife.carmanagement.addcar.AddCarActivity"></activity>
        <activity android:name=".search.search_home.SearchPagerFragment"></activity>
        <activity android:name=".SettingActivity"></activity>
        <activity android:name="com.happylife.carmanagement.modifycar.ModifyCarActivity"></activity>
        <activity android:name=".search.search_do.SearchActivity"></activity>

        <service
            android:name=".common.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>